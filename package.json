{
  "name": "moggy",
  "version": "0.1.0",
  "description": "Presenting a foolish implementation of immutability that aspires to ignore the most cherished taboo of not modifying prototypes for unspecified reasons.",
  "main": "dist/moggy.js",
  "scripts": {
    "build": "webpack -p && gzip < dist/moggy.js > dist/moggy.js.gz",
    "test": "npm run size && ava",
    "size": "fslint --files=dist/moggy.js.gz --limit=1024"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/Transmogrify.git"
  },
  "keywords": [
    "immutable",
    "mutable",
    "mutability",
    "immutability",
    "functional",
    "moggies",
    "moggy",
    "mog"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Wildhoney/Transmogrify/issues"
  },
  "homepage": "https://github.com/Wildhoney/Transmogrify#readme",
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.22.0",
    "fslint": "^1.1.0",
    "webpack": "^1.14.0"
  }
}
